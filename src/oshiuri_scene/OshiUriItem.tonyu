extends BaseHireiFollItem;
super.main();

tarX;
vx = 1;
dist = x + -( tarX );
invincible;
hp;

while( true ){
    x = tarX + ( dist / vx );
    vx += 1;
    foll();
    
    bul = crashTo( Bullet );
    if( bul && !invincible ) {
        bul.die();
        hp--;
        if( hp <= 0 ) die();
        $sound.playSE( $se_gua );
    };

    update();
}