native Math;
frame = 0;
scrScale = 0.3338;
W = Math.floor( 1080 * scrScale );
H = Math.floor( 1920 * scrScale );

$Screen.resize( W, H );

new Actor{
    x=W * 0.5, y=H * 0.5, 
    p=$pat_ojisan_title, scaleX=scrScale,
    zOrder=100,
};

while( true ){
    frame++;
    
    if( $touches[0].touched ){
        loadPage( Start );
    }
    
    update();
};